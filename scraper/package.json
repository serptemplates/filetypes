{
  "name": "filetypes-scraper",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "fetch": "bash scripts/fetch.sh",
    "parse": "node scripts/parse-sources.mjs",
    "merge": "node scripts/merge.mjs",
    "enrich:mime": "node scripts/enrich-mime.mjs",
    "build:mime": "node scripts/build-mime.mjs",
    "integrate:mime": "node scripts/integrate-mime.mjs",
    "augment:mime:iana": "node scripts/augment-mime-iana.mjs",
    "fetch:mime:iana": "node scripts/fetch-iana-mime.mjs",
    "fetch:mime:mappings": "node scripts/fetch-mime-mappings.mjs",
    "build:signatures": "node scripts/build-signatures.mjs",
    "integrate:signatures": "node scripts/integrate-signatures.mjs",
    "build:matrix": "node scripts/build-matrix.mjs",
    "fetch:rfc": "bash scripts/fetch-rfc.sh",
    "build:rfc": "node scripts/build-rfc.mjs",
    "integrate:rfc": "node scripts/integrate-rfc.mjs",
    "build": "npm run parse && npm run merge && npm run enrich:mime && node scripts/combine.mjs",
    "normalize:filetypes": "node scripts/normalize-filetypes.mjs",
    "gen:exts": "node scripts/gen-extensions-from-mime-db.mjs data/extensions.txt",
    "gen:exts:scan": "node scripts/gen-extensions-scan-raw.mjs data/extensions.txt",
    "all": "npm run fetch && npm run build",
    "inventory:fields": "node scripts/inventory-fields.mjs",
    "fetch:zyte": "python3 scripts/fetch_zyte_api.py"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "mime-db": "^1.53.0",
    "yaml": "^2.5.0"
  }
}
